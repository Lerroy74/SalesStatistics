
@{
    ViewData["Title"] = "Upload";
}

<h2>Upload</h2>

<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="UploadFiles" style="margin: 50px">
    <div class="form-group">
        <div class="col-md-10">
            <p>Форма загрузки excel файла:</p>
            <input type="file" name="files" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>